<!doctype html>
<html class="no-js" lang="{{ request.locale.iso_code }}" style="--wHeightPx: 100vh;">
<head>
	<meta charset="UTF-8">
	<meta name="viewport" content="width=device-width, initial-scale=1">
	{%- if request.page_type contains 'customers' or request.page_type contains 'cart' or request.page_type contains 'account' or request.page_type contains 'blogs' or request.page_type contains 'captcha' -%}
	<meta name="robots" content="noindex" />
	{%- endif -%}
	<link rel="canonical" href="{{ canonical_url }}">
	<meta name="google-site-verification" content="1_P1Xhv4k9Dr-1gcr7NUMd_dsqVRuE3w-AcSwmrMEj8" />
	<link rel="shortcut icon" type="image/x-icon" href="https://journal.komons-japan.com/wp-content/themes/komons-theme/img/icon/icon.jpg">
	<link rel="apple-touch-icon" href="https://journal.komons-japan.com/wp-content/themes/komons-theme/img/icon/icon.jpg" sizes="200x200">
	<meta name="google-site-verification" content="hmx_ejVjHWOZdBvcBBZJDO223RAXBT-8VFHv1_SuNdg" />
	<meta name="facebook-domain-verification" content="n1740khqc61uch3sbafw6md4c74g7p" />
	{{ 'style.css' | asset_url | stylesheet_tag }}
	{%- if request.page_type contains 'account' -%}
	<style>body.huckleberry-subscription-list-color{display: flex;align-items: center;justify-content: center;min-height: calc(100vh - 120px);max-height: unset;}#root {width: 100%;font-family: YakuHanMP, "ヒラギノ明朝 ProN W3", "Hiragino Mincho ProN", "游明朝", YuMincho, "HG明朝E", "ＭＳ Ｐ明朝", "ＭＳ 明朝", "Sawarabi Mincho", serif !important;}</style>
	{{ 'subscription.css' | asset_url | stylesheet_tag }}
	{%- endif -%}
  {%- capture seo_title -%}
    {%- if request.page_type == 'search' and search.performed == true -%}
			{%- comment -%}
			{{ 'general.search.heading' | t: count: search.results_count }}: {{ 'general.search.results_with_count' | t: terms: search.terms, count: search.results_count }}
			{%- endcomment -%}
			{%- comment -%}
			{{ 'general.search.results_with_count' | t: terms: search.terms, count: search.results_count }}
			{%- endcomment -%}
			{%- comment -%}
			{{ 'general.search.results' | t: terms: search.terms }}
			{%- endcomment -%}
			{%- assign search_message = 'の検索結果 | 毎日の家事を、心地よい時間に変えるホームケアブランド - Komons(コモンズ)' -%}
			{{ search.terms }}{{ search_message }}
    {%- else -%}
      {{ page_title }}
    {%- endif -%}
    {%- if current_tags -%}
      {%- assign meta_tags = current_tags | join: ', ' -%} &ndash; {{ 'general.meta.tags' | t: tags: meta_tags -}}
    {%- endif -%}
    {%- if current_page != 1 -%}
      &ndash; {{ 'general.meta.page' | t: page: current_page }}
    {%- endif -%}
    {%- assign escaped_page_title = page_title | escape -%}
    {%- unless escaped_page_title contains shop.name -%}
      &ndash; {{ shop.name }}
    {%- endunless -%}
  {%- endcapture -%}
	{%- if request.page_type contains 'index' -%}
	<title>【公式】Komons (コモンズ) | 毎日の家事を、心地よい時間に変えるホームケアブランド</title>
	{%- else -%}
	<title>{{ seo_title }}</title>
	{%- endif -%}
  {%- if page_description -%}
    <meta name="description" content="{{ page_description | escape }}">
  {%- endif -%}
  {% include 'social-meta-tags' %}
  <meta name="robots" content="index">
  <meta name="robots" content="follow">
	{%- if request.page_type contains 'index' -%}
	<link rel="preload"
  as="image" href="https://res.cloudinary.com/dbwqcy0op/image/upload/f_auto,q_auto/v1583750620/index/main_slide01_sp_dzdwrd.jpg"
  imagesizes="100vw"/>
	<link rel="preload"
	as="image" href="https://res.cloudinary.com/dbwqcy0op/image/upload/f_auto,q_auto/v1583750620/index/main_slide02_sp_bphwgj.jpg"
	imagesizes="100vw"/>
	{%- endif -%}
	{%- if request.page_type contains 'index' or request.page_type contains 'product' or request.page_type contains 'collection' -%}
	{%- else -%}
	<script type="text/javascript" src="//cdn.shopify.com/s/files/1/0536/9544/7234/t/2/assets/jquery-3.3.1.min.js" charset="UTF-8"></script>
	{%- endif -%}
	<script>
		function send_conversion() {
			$('#cartSubmit').click();
		}
	</script>

	{{ content_for_header }}
</head>
{%- if request.page_type contains 'captcha' -%}
<body class="fixed">
	{%- else -%}
<body class="{{ request.page_type }}">
	{%- endif -%}
	{%- if request.page_type contains 'account' -%}
	{%- else -%}
	{% section 'header' %}
	{%- endif -%}
	{{content_for_layout}}
	{%- if request.page_type contains 'account' -%}
	{% section 'header' %}
	{%- endif -%}
	{% section 'footer' %}
	{% section 'slide-menu' %}
</body>
<link rel="stylesheet" href="https://fonts.googleapis.com/css?family=EB+Garamond:400,500">
<link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/yakuhanjp@3.4.1/dist/css/yakuhanmp.min.css">
